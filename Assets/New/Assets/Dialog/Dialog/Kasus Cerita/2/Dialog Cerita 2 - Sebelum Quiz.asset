    GambarKarakter: {fileID: 21300000, guid: c12acb0a7daac894185877a1cff3c268, type: 3}
    Nama: 12
    sentences: Setuju sih, apa lagi disini kan banyak ciwi-ciwi nih hehehe.
    Kiri: 1
  - narasi:
      narasiText: 
      VO: {fileID: 0}
    GambarKarakter: {fileID: 21300000, guid: d192c49cb2cb62d4ab5f0b5261086e47, type: 3}
    Nama: 8
